        var a = 3;
        var b = a++;
        console.log(b);
        console.log(a);
        
        var c = 3;
        var d = ++c;
        console.log(d);
        console.log(c);

        var a = 3;
        var b = 4;
        alert(a++ + b++ + ++a + ++b);
    </script>
